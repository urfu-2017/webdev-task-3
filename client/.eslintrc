{
    "extends": "hrundel/browser",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "no-return-assign": "off",
        "no-invalid-this": "off"
    }
}
